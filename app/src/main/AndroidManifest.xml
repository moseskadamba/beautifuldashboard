<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.speechpadgeneralguidelines">

    <application
        android:allowBackup="true"
        android:icon="@drawable/speechpadlogo1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".OpeningPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".GeneralText" />
        <activity android:name=".FillerWords" />
        <activity android:name=".SpeakerID" />
        <activity android:name=".InaudibleAcitivity" />
        <activity android:name=".UtteranceActivity" />
        <activity android:name=".TimestampActivity" />
        <activity android:name=".ValidationActivity" />
        <activity android:name=".GrammerActivity"></activity>
    </application>

</manifest>